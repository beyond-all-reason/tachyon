        "source": "server",
        "target": "user",
        "scopes": ["tachyon.lobby"]
    },
    "type": "object",
    "properties": {
        "type": { "const": "event" },
        "messageId": { "type": "string" },
        "commandId": { "const": "clan/updateList" },
        "data": {
            "title": "ClanUpdateListEventData",
            "type": "object",
            "properties": {
                "clans": {
                    "type": "array",
                    "items": { "$ref": "../../definitions/clan.json" }
                }
            },
            "required": ["clans"]
        }
    },
    "required": ["type", "messageId", "commandId", "data"]
}
