{
    "$id": "https://schema.beyondallreason.dev/tachyon/clan/getList/response.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ClanGetListResponse",
    "tachyon": {
        "source": "server",
        "target": "user",
        "scopes": ["tachyon.lobby"]
    },
    "anyOf": [
        {
            "title": "ClanGetListOkResponse",
            "type": "object",
            "properties": {
                "type": { "const": "response" },
                "messageId": { "type": "string" },
                "commandId": { "const": "clan/getList" },
                "status": { "const": "success" },
                "data": {
                    "title": "ClanGetListOkResponseData",
                    "type": "object",
                    "properties": {
                        "clans": {
                            "type": "array",
                            "items": { "$ref": "../../definitions/clan.json" }
                        }
                    },
                    "required": ["clans"]
                }
            },
            "required": ["type", "messageId", "commandId", "status", "data"]
        },
        {
            "title": "ClanGetListFailResponse",
            "type": "object",
            "properties": {
                "type": { "const": "response" },
                "messageId": { "type": "string" },
                "commandId": { "const": "clan/getList" },
                "status": { "const": "failed" },
                "reason": {
                    "enum": [
                        "internal_error",
                        "unauthorized",
                        "invalid_request",
                        "command_unimplemented"
                    ]
                },
                "details": { "type": "string" }
            },
            "required": ["type", "messageId", "commandId", "status", "reason"]
        }
    ]
}
