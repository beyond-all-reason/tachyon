{
    "$id": "https://schema.beyondallreason.dev/tachyon/clan/view/request.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ClanViewRequest",
    "tachyon": {
        "source": "user",
        "target": "server",
        "scopes": ["tachyon.lobby"]
    },
    "type": "object",
    "properties": {
        "type": { "const": "request" },
        "messageId": { "type": "string" },
        "commandId": { "const": "clan/view" },
        "data": {
            "title": "ClanViewRequestData",
            "type": "object",
            "properties": {
                "clanId": { "$ref": "../../definitions/clanId.json" },
                "userIds": {
                    "type": "array",
                    "items": { "$ref": "../../definitions/userId.json" }
                }
            },
            "required": ["clanId", "userIds"]
        }
    },
    "required": ["type", "messageId", "commandId", "data"]
}
